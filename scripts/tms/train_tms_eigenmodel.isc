    --n-training-datapoints 10000   --n-eval-datapoints 1000  --sparsity 0.05 \
    --lr .001   --batch-size 32    \
    --lr-step-epochs 10  --lr-decay-rate 0.8 \
    --epochs 2000 --log-epochs 100 --checkpoint-epochs 200
