source ~/.eigenestimation/bin/activate && 
cd ~/eigenestimation/ && 
rm outputs/eigenmodels/tms_parallel.pt &&
torchrun  --nnodes=$NNODES --nproc-per-node=$N_PROC  scripts/tms/train_tms_eigenmodel.py  \
